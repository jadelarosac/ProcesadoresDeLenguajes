programa_principal(){

	declarar
	entero n, curr;
	fin declarar
	
	escribir "introduce numero :";
	leer n;
	curr = 2;

	mientras (curr <= n){
		declarar
			entero d;
		fin declarar
		d= n/curr;

		si (d*curr == n){
			escribir "* ";
			n = n/curr;
		} 
		si no 
			curr = curr + 1;
	}
	escribir "\n";
}
