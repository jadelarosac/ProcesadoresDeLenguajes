programa_principal(){
  declarar
	real v[5],res[5];
  	entero n;
  fin declarar
  v = [0.5,0.9,0.99,0.999,1.00001];

  res = v;

  escribir "v1: ",v[0],"v2: ",v[1],"v3: ",v[2],"v4: ",v[3],"v5: ",v[4],"\n"; 
  n = 0;
  
  mientras (n<20){
  	v = v*v;
  	escribir "v1: ",v[0],"v2: ",v[1],"v3: ",v[2],"v4: ",v[3],"v5: ",v[4],"\n";
  	n = n + 1; 
  }
}
